<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id = "mobile" class = "com.shadab.demo.bean.MobileBean">
      <property name = "mobilePrice" value = "250"/>
      <property name = "mobileHeight" value = "6"/>
      <property name = "mobileWeight" value = "200"/>
      <property name = "mobileCompany" value = "Samsung"/>
      <property name = "mobileModel" value = "Galaxy"/>
   </bean>

</beans>